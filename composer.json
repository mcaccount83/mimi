{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "almasaeed2010/adminlte": "~3.2",
        "authorizenet/authorizenet": "^2.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "dcblogdev/laravel-sent-emails": "^2.0",
        "google/apiclient": "^2.18",
        "google/cloud-recaptcha-enterprise": "^2.2",
        "guzzlehttp/guzzle": "^7.10",
        "laravel/framework": "^12.48",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.6",
        "league/flysystem-aws-s3-v3": "^3.30",
        "livewire/livewire": "^3.7",
        "rap2hpoutre/laravel-log-viewer": "^2.5",
        "riari/laravel-forum": "^7.0",
        "romanzipp/laravel-queue-monitor": "^5.4",
        "setasign/fpdf": "^1.8",
        "setasign/fpdi": "^2.6",
        "spatie/laravel-html": "^3.12",
        "spatie/laravel-sitemap": "^7.3"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.25",
        "laravel/sail": "^1.48",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.8",
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^12.4",
        "spatie/laravel-ignition": "^2.9",
        "laravel/pail": "^1.2.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "kylekatarnls/update-helper": true
        }
    }
}
